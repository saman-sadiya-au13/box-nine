<div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
            <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
            <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
        <img class="d-block w-100" width="100%" height="600px"  src="/images/imageone.jpeg" alt="Third slide">
        <div class="carousel-caption d-none d-md-block">
            <h1>Welcome to Box9</h1>
        </div>
        </div>
        <div class="carousel-item">
        <img class="d-block w-100" width="100%" height="600px"  src="/images/image6.jpeg" alt="Second slide">
        </div>
        <div class="carousel-item">
        <img class="d-block w-100" width="100%" height="600px" src="/images/image5.jpeg" alt="First slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<main class="container">
    <div class="row">

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s" id="forcolor">
                <div class="card-body">
                    <i class="fas fa-check"></i>
                    <h1 class="card-title pricing-card-title">{{delivered}} <small class="text-muted">Order completed</small></h1>
                </div>
            </div>
        </div>

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-spinner"></i>
                    <h1 class="card-title pricing-card-title">{{processing}} <small class="text-muted">Orders Processing</small></h1>
                </div>
            </div>
        </div>

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-bullhorn"></i>
                    <h1 class="card-title pricing-card-title">{{recieved}} <small class="text-muted">Orders Recieved</small></h1>
                </div>
            </div>
        </div>

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-exclamation-triangle"></i>
                    <h1 class="card-title pricing-card-title">{{cancelled}} <small class="text-muted">Orders Cancelled</small></h1>
                </div>
            </div>
        </div>

        <div class="col-12">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-calendar-check"></i>
                    <h1 class="card-title pricing-card-title">{{totalOrder}} <small class="text-muted">Orders for this month</small></h1>
                </div>
            </div>
        </div>

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-wallet"></i>
                    <h1 class="card-title pricing-card-title">0 <small class="text-muted">Revenu for this month</small></h1>
                </div>
            </div>
        </div>

        <div class="col-3">
            <div class="card mb-4 mt-4 box-shadow card-s">
                <div class="card-body">
                    <i class="fas fa-rupee-sign"></i>
                    <h1 class="card-title pricing-card-title">0 <small class="text-muted">Today's Revenu</small></h1>
                </div>
            </div>
        </div>
        
    </div>
</main>
<script src="/javascripts/admin.js"></script>
